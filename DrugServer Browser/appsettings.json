{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FDBDrugServerSettings": {
    "ConnectionStrings": {
      "DotNetTest": "Data Source=(Local);Initial Catalog=DotNetTest;Integrated Security=True;"
    },
    "Localisation": {
      "DefaultLanguage": "English",
      "DefaultRegion": "England"
    },
    "Database": {
      "ConnectionName": "DotNetTest",
      "MaxRetryCount": 3,
      "RetrySleepInMilliseconds": 100
    },
    "Dose": {
      "DisplayFormat_DateAndTime": "ddd dd-MMM-yyyy HH:mm",
      "DisplayFormat_DateOnly": "ddd dd-MMM-yyyy",
      "DisplayFormat_TimeOnly": "HH:mm"
    } 
  }
}
